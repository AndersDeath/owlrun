{
  "name": "owlrun",
  "author": {
    "name": "Vladimir Bolshakov",
    "url": "https://github.com/AndersDeath/owlrun",
    "email": "andersdeath@gmail.com"
  },
  "version": "0.0.5",
  "description": "Simple cli for fast starters installing",
  "main": "index.js",
  "keywords": [
    "cli",
    "starter",
    "boilerplate",
    "typescript",
    "spa",
    "react",
    "angular",
    "nodejs",
    "javascript",
    "webpack"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:js": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline && node postbuild.js",
    "cli:run": "npm run build && node ./dist/index.js",
    "compodoc": "npx compodoc -p tsconfig.json",
    "test:run": "npx jest",
    "test:coverage": "npx jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndersDeath/owlrun"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "execa": "^4.0.0",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^26.0.5",
    "@types/node": "^13.7.4",
    "@types/yargs": "^15.0.5",
    "jest": "^26.1.0",
    "jest-diff": "^26.1.0",
    "pretty-format": "^26.1.0",
    "ts-jest": "^26.1.3",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.7"
  },
  "bin": {
    "owlrun": "./dist/index.js"
  }
}
