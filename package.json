{
  "name": "owlrun",
  "author": {
    "name": "Vladimir Bolshakov",
    "url": "https://github.com/AndersDeath/owlrun",
    "email": "andersdeath@gmail.com"
  },
  "version": "0.0.3",
  "description": "Simple cli for fast starters installing",
  "main": "index.js",
  "keywords": [
    "cli",
    "starter",
    "boilerplate",
    "typescript",
    "spa",
    "react",
    "angular",
    "nodejs",
    "javascript"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline && node postbuild.js",
    "cli:run": "npm run build && node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndersDeath/owlrun"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "execa": "^4.0.0",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.7.4",
    "babel-loader": "^8.1.0",
    "typescript": "^3.8.2"
  },
  "bin": {
    "owlrun": "./dist/index.js"
  }
}
