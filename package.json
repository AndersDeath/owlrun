    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "webpack --mode=production ",
    "cli:run": "npm run build && node ./dist/index.js"
  },
  "repository": {
    "type": "git"
  },
