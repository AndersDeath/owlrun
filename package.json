    "figlet": "^1.2.4",
    "inquirer": "^7.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.7.4",
    "babel-loader": "^8.1.0",
    "typescript": "^3.8.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
